{
    "name": "deniztezcan/laravel-sendcloud-shipping",
    "description": "Laravel Wrapper for the SendCloud Shipping API",
    "type": "library",
    "license": "MIT",
    "version": "0.2.0",
    "homepage": "https://github.com/deniztezcan/laravel-sendcloud-shipping",
    "authors": [
        {
            "name": "Deniz Tezcan",
            "email": "howdy@deniztezcan.me"
        },
        {
            "name": "Stephan Groen",
            "email": "info@picqer.com"
        }
    ],
     "keywords": [
        "api",
        "php",
        "sendcloud",
        "laravel"
    ],
    "autoload": {
        "psr-4": { "DenizTezcan\\SendCloud\\" : "src/" }
    },
    "minimum-stability": "stable",
    "require": {
        "illuminate/support": "5.4.*|5.5.*|5.6.*|5.7.*|5.8.*|6.*.*",
        "guzzlehttp/guzzle": "~6.0"
    },
    "extra": {
      "laravel": {
        "providers": [
          "DenizTezcan\\SendCloud\\SendCloudServiceProvider"
        ],
        "aliases": {
          "SendCloud": "DenizTezcan\\SendCloud\\Facades\\SendCloud"
        }
      }
    }
}
